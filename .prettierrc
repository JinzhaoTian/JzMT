{
    "plugins": [
        "prettier-plugin-tailwindcss",
        "prettier-plugin-organize-imports"
    ],
    "tailwindFunctions": ["classNames"],
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": true,
    "useTabs": false,
    "trailingComma": "none",
    "singleQuote": true,
    "endOfLine": "lf",
    "tabWidth": 4
}
